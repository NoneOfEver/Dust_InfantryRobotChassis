{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "InfantryRobotChassis DAPlink",
            "cwd": "${workspaceRoot}",
            "executable": "${workspaceRoot}/build/dm-h723-DownBoard_Interaction/FW.elf",
            "request": "launch",
            "type": "cortex-debug",
            "device": "STM32H723VG",
            "svdFile": "${workspaceRoot}/Board/dm-h723/STM32H723.svd",
            "servertype": "openocd",
            "configFiles": [
                "${workspaceRoot}/Board/dm-h723/config/openocd_dap.cfg",
            ],
            "runToEntryPoint": "main",
        },
        {
            "name": "InfantryRobotChassis Jlink",
            "cwd": "${workspaceFolder}",
            "executable": "${workspaceRoot}/build/dm-h723-DownBoard_Interaction/FW.elf",
            "request": "launch",
            "type": "cortex-debug",
            "device": "STM32H723VG",
            "runToEntryPoint": "main",
            "showDevDebugOutput": "none",
            "servertype": "jlink",
            "interface": "swd",
            "svdFile": "${workspaceRoot}/Board/dm-h723/STM32H723.svd",
            "rtos": "FreeRTOS",
        
        },
        // Attach configurations
        {
            "name": "InfantryRobotChassis DAP-attach",
            "cwd": "${workspaceRoot}",
            "executable": "${workspaceRoot}/build/dm-h723-DownBoard_Interaction/FW.elf",
            "request": "attach",
            "type": "cortex-debug",
            "device": "STM32H723VG",
            "svdFile": "${workspaceRoot}/Board/dm-h723/STM32H723.svd",
            "servertype": "openocd",
            "configFiles": [
                "openocd_dap.cfg",
            ],
        },
        {
            "name": "InfantryRobotChassis Jlink-attach",
            "cwd": "${workspaceFolder}",
            "executable": "${workspaceRoot}/build/dm-h723-DownBoard_Interaction/FW.elf",
            "request": "attach",
            "type": "cortex-debug",
            "device": "STM32H723VG",
            "showDevDebugOutput": "none",
            "servertype": "jlink",
            "interface": "swd",
            "svdFile": "${workspaceRoot}/Board/dm-h723/STM32H723.svd",
            "rtos": "FreeRTOS",
        },
    ],
}